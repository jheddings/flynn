<?xml version="1.0" encoding="utf-8" ?>

<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System" >
      <section name="Flynn.X10.Properties.Settings" type="System.Configuration.ClientSettingsSection, System" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <applicationSettings>
    <Flynn.X10.Properties.Settings>
      <setting name="SendAsync" serializeAs="String">
        <value>True</value>
      </setting>

      <setting name="CM17A_ResetHoldMs" serializeAs="String">
        <value>250</value>
      </setting>
      <setting name="CM17A_BitHoldMs" serializeAs="String">
        <value>2</value>
      </setting>
      <setting name="CM17A_SendDelayMs" serializeAs="String">
        <value>500</value>
      </setting>

      <setting name="SmartLinc_SendDelayMs" serializeAs="String">
        <value>500</value>
      </setting>
    </Flynn.X10.Properties.Settings>
  </applicationSettings>
</configuration>
